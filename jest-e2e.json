{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testEnvironment": "node",
  "rootDir": "./",
  "testRegex": ".test-e2e.ts$",
  "moduleNameMapper": {
    "^@@app/(.*)": "<rootDir>/src/application/$1",
    "^@@domain/(.*)": "<rootDir>/src/domain/$1",
    "^@@infra/(.*)": "<rootDir>/src/infra/$1",
    "^@@test/*": ["<rootDir>/src/test/$1"]

  },
  "preset": "ts-jest"
}
